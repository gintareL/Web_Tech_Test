POST	/ruecken/Uebung/Auswaehlen		controllers.Application.auswaehlenRuecken()
POST	/bauch/Uebung/Auswaehlen		controllers.Application.auswaehlenBauch()
POST	/schultern/Uebung/Auswaehlen		controllers.Application.auswaehlenSchultern()
POST	/brust/Uebung/Auswaehlen		controllers.Application.auswaehlenBrust()
POST	/uebungLoeschenBrust		controllers.Application.uebungLoeschenBrust()
POST	/uebungLoeschenBeine		controllers.Application.uebungLoeschenBeine()
POST	/uebungLoeschenBauch		controllers.Application.uebungLoeschenBauch()
POST	/uebungLoeschenArme		controllers.Application.uebungLoeschenArme()
POST	/uebungLoeschenSchultern		controllers.Application.uebungLoeschenSchultern()
POST	/uebungLoeschenRuecken		controllers.Application.uebungLoeschenRuecken()
