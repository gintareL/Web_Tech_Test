# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /            	       controllers.Application.index()
GET		/atGym					controllers.Application.login()
GET		/home					controllers.Application.home()
POST	/home					controllers.Application.home1()
GET		/ourGym					controllers.Application.ourGym()
GET 	/arme					controllers.Application.arme()
GET 	/beine					controllers.Application.beine()
GET 	/bauch					controllers.Application.bauch()
GET 	/brust					controllers.Application.brust()
GET		/myGym					controllers.Application.myGym()
GET		/ruecken				controllers.Application.ruecken()
GET		/schultern				controllers.Application.schultern()
GET 	/aboutMe				controllers.Application.aboutMe()
POST	/aboutMe/Gewicht			controllers.Application.aboutMeg()
POST	/aboutMe/Bauchumfang		controllers.Application.aboutMeb()
POST	/aboutMe/Hueftenumfang		controllers.Application.aboutMeh()
POST	/aboutMe/Armumfang			controllers.Application.aboutMea()
POST	/aboutMe/Brustumfang		controllers.Application.aboutMebr()
POST	/uebungLoeschen				controllers.Application.uebungLoeschen()
POST	/uebungAuswaehlen			controllers.Application.uebungAuswaehlen()
POST	/foto						controllers.Application.upload()
POST	/satzSave					controllers.Application.satzSave()
POST	/like						controllers.Application.like()
GET		/likesWS					controllers.Application.uebungWebSocket()
POST	/planLoeschen			controllers.Application.planLoeschen()
GET 	/myPlans				controllers.Application.myPlans()
GET 	/myRoutine				controllers.Application.myRoutine()
GET 	/myAnalyse				controllers.Application.myAnalyse()
GET		/plannameVervollstaendigung/		controllers.Application.plannameVervollstaendigung(input)
#POST	/upload/save			controllers.Application.upload()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file       controllers.Assets.at(path="/public", file)
